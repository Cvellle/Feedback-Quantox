"source":"src/assets/scripts/modules/roadmap.js","name":"columnSwitchBtns","original":{"line":131,"column":2},"generated":{"line":1,"column":2944}},{"source":"src/assets/scripts/modules/roadmap.js","name":"columnSwitchBtns","original":{"line":132,"column":4},"generated":{"line":1,"column":2947}},{"source":"src/assets/scripts/modules/roadmap.js","name":"forEach","original":{"line":132,"column":21},"generated":{"line":1,"column":2949}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":132,"column":29},"generated":{"line":1,"column":2957}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":132,"column":30},"generated":{"line":1,"column":2966}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":133,"column":6},"generated":{"line":1,"column":2969}},{"source":"src/assets/scripts/modules/roadmap.js","name":"addEventListener","original":{"line":133,"column":14},"generated":{"line":1,"column":2971}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":133,"column":31},"generated":{"line":1,"column":2988}},{"source":"src/assets/scripts/modules/roadmap.js","name":"switchColumns","original":{"line":133,"column":40},"generated":{"line":1,"column":2996}}],"sources":{"src/assets/scripts/modules/roadmap.js":"import { tabletMin } from \"../shared/constants\";\r\nimport { addItemDetailsListener, feedbackDetails, filterStatus } from \"../shared/shared-functions\";\r\nimport { initialValues } from \"./getSuggestions\";\r\n\r\nexport function roadmapLists() {\r\n  const roadmapFeedbackWrapper = document.querySelector(\r\n    \".feedback--roadmap .feedback-items-wrapper\"\r\n  );\r\n  const feedbackItems = document.querySelectorAll(\".feedback-item\");\r\n  const roadmapColumnsWrapper = document.querySelector(\r\n    \".roadmap .roadmap__columns\"\r\n  );\r\n  \r\n\r\n  const columns = [\r\n    {\r\n      name: \"planned\",\r\n      visible: true,\r\n    },\r\n    {\r\n      name: \"in-progress\",\r\n      visible:  tabletMin,\r\n    },\r\n    {\r\n      name: \"live\",\r\n      visible:  tabletMin,\r\n    },\r\n  ];\r\n\r\n  let shown = [];\r\n  const all = initialValues.feedbackArray;\r\n\r\n  const filterInMap = (toFilter) =>\r\n    toFilter.map((el) => {\r\n      return `<div class=\"feedback-item\" id=\"${el.id}\">\r\n                <div class=\"feedback-item__left\">\r\n                  <span class=\"arrow\"></span>\r\n                  <div class=\"count\">${el.upvotes}</div>\r\n                </div>\r\n                <div class=\"feedback-item__center\">\r\n                  <h4 class=\"title\">${el.title}</h4>\r\n                  <p>\r\n                    ${el.description}\r\n                  </p>\r\n                  <div class=\"tag\">\r\n                    <span>${el.category}</span>\r\n                  </div>\r\n                </div>\r\n                <div class=\"feedback-item__right\">\r\n                  <span class=\"comment\"></span>\r\n                  <div class=\"count\">\r\n                    ${el.comments ? el.comments.length : 0}\r\n                  </div>\r\n                </div>\r\n            </div>`;\r\n    });\r\n\r\n  const columnsLists = () => {\r\n    let mapped = columns.map((el, i, self) => {\r\n      \r\n      shown.push(all.filter((f) => f.status == el.name));\r\n\r\n      const visibleColumn = el.visible\r\n        ? `\r\n      <div class=\"roadmap__column roadmap__column--${el.name}\">\r\n        <bold>\r\n          ${el.name}\r\n          <span>(<span class=\"count\">${shown[i].length}</span>)</span>\r\n        </bold>\r\n        <div>In search</div>\r\n        <div>\r\n          <div class=\"item\">\r\n            <div><span></span></div>\r\n          </div>\r\n        </div>\r\n        <div class=\"feedback feedback--roadmap\">\r\n          <div class=\"feedback-items-wrapper\">\r\n            ${el.visible && filterInMap(shown[i]).join(\"\")}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    `\r\n        : null;\r\n\r\n      const mobileHeader = self.map((buttonEl, buttonIndex) => {\r\n        let allColumns = filterStatus(true);\r\n        return `\r\n        <div class=\"btn-name\">\r\n          <bold>\r\n            <span>${buttonEl.name}</span>\r\n            <span>(<span class=\"count\">${allColumns[buttonIndex].length}</span>)</span>\r\n          </bold>\r\n        </div> `;\r\n      });\r\n\r\n      return el.visible\r\n        ? `\r\n      <div class=\"roadmap-mobile-header\">\r\n        ${mobileHeader.join(\"\")}  \r\n      </div>\r\n    ` + visibleColumn\r\n        : null;\r\n    });\r\n\r\n    return mapped.join(\"\");\r\n  };\r\n\r\n  const switchColumns = (e) => {\r\n    let current = columns.find(\r\n      (el) =>\r\n        el.name == e.currentTarget.firstElementChild.firstElementChild.innerHTML\r\n    );\r\n    columns.forEach((el) => (el.visible = false));\r\n    current.visible = true;\r\n    roadmapColumnsWrapper.innerHTML = columnsLists();\r\n    const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\r\n\r\n    columnSwitchBtns.forEach((element) => {\r\n      element.addEventListener(\"click\", switchColumns);\r\n    });\r\n    e.currentTarget.classList.add(\"btn-name--active\");\r\n    // add event listeners after repiant\r\n    addItemDetailsListener()\r\n  };\r\n\r\n  roadmapColumnsWrapper.innerHTML = columnsLists();\r\n\r\n  // Mobile switch columns\r\n  const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\r\n\r\n  columnSwitchBtns &&\r\n    columnSwitchBtns.forEach((element) => {\r\n      element.addEventListener(\"click\", switchColumns);\r\n    });\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"cd32b22df9e427e115285c36132c0198","cacheData":{"env":{}}}