ource":"src/assets/scripts/modules/roadmap.js","name":"columnSwitchBtns","original":{"line":148,"column":4},"generated":{"line":1,"column":3159}},{"source":"src/assets/scripts/modules/roadmap.js","name":"forEach","original":{"line":148,"column":21},"generated":{"line":1,"column":3161}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":148,"column":29},"generated":{"line":1,"column":3169}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":148,"column":30},"generated":{"line":1,"column":3178}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":149,"column":6},"generated":{"line":1,"column":3181}},{"source":"src/assets/scripts/modules/roadmap.js","name":"addEventListener","original":{"line":149,"column":14},"generated":{"line":1,"column":3183}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":149,"column":31},"generated":{"line":1,"column":3200}},{"source":"src/assets/scripts/modules/roadmap.js","name":"switchColumns","original":{"line":149,"column":40},"generated":{"line":1,"column":3208}}],"sources":{"src/assets/scripts/modules/roadmap.js":"import { tabletMin } from \"../shared/constants\";\r\nimport {\r\n  addItemDetailsListener,\r\n  feedbackDetails,\r\n  filterStatus,\r\n} from \"../shared/shared-functions\";\r\nimport { getLS, initialValues } from \"./getSuggestions\";\r\n\r\nexport function roadmapLists() {\r\n  const roadmapFeedbackWrapper = document.querySelector(\r\n    \".feedback--roadmap .feedback-items-wrapper\"\r\n  );\r\n  const feedbackItems = document.querySelectorAll(\".feedback-item\");\r\n  const roadmapColumnsWrapper = document.querySelector(\r\n    \".roadmap .roadmap__columns\"\r\n  );\r\n\r\n  const columns = [\r\n    {\r\n      name: \"planned\",\r\n      visible: true,\r\n    },\r\n    {\r\n      name: \"in-progress\",\r\n      visible: tabletMin,\r\n    },\r\n    {\r\n      name: \"live\",\r\n      visible: tabletMin,\r\n    },\r\n  ];\r\n\r\n  let shown = [];\r\n  const all = getLS(\"feedbackArray\");\r\n\r\n  const filterInMap = (toFilter) =>\r\n    toFilter.map((el, i) => {\r\n      return `\r\n      <section class=\"feedback-item\" id=\"${el.id}\">\r\n        <div class=\"feedback-item__left\">\r\n          <div class=\"upvotes ${\r\n            el.likedBy &&\r\n            el.likedBy.includes(getLS(\"currentUser\").name)\r\n              ? \"upvotes--highlighted\"\r\n              : \"\"\r\n          }\">\r\n            <span class=\"arrow\"></span>\r\n            <input type=\"hidden\" />\r\n            <div class=\"count\">${el.upvotes}</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"feedback-item__center\">\r\n          <h4 class=\"title\">${el.title}</h4>\r\n          <p>${el.description}</p>\r\n          <div class=\"tag\">\r\n            <span>${el.category}</span>\r\n          </div>\r\n        </div>\r\n        <div class=\"feedback-item__right\">\r\n          <span class=\"comment\"></span>\r\n          <div class=\"count\">\r\n            ${el.comments ? el.comments.length : 0}\r\n          </div>\r\n        </div>\r\n      </section>\r\n      `;\r\n    });\r\n\r\n  const mobileHeader = () => {\r\n    let header = columns.map((buttonEl, buttonIndex) => {\r\n    let allColumns = filterStatus(true);\r\n    return `\r\n      <div class=\"btn-name ${buttonEl.visible ? 'btn-name--active' : ''}\">\r\n        <bold>\r\n          <span>${buttonEl.name}</span>\r\n          <span>(<span class=\"count\">${allColumns[buttonIndex].length}</span>)</span>\r\n        </bold>\r\n      </div> `;\r\n    });  \r\n\r\n    return header.join('')\r\n  }\r\n\r\n  const columnsLists = () => {\r\n    let mapped = columns.map((el, i, self) => {\r\n      shown.push(all.filter((f) => f.status == el.name));\r\n\r\n      const visibleColumn = el.visible\r\n        ? `\r\n          <div class=\"roadmap__column roadmap__column--${el.name}\">\r\n            <bold>\r\n              ${el.name}\r\n              <span>(<span class=\"count\">${shown[i].length}</span>)</span>\r\n            </bold>\r\n            <div>In search</div>\r\n            <div>\r\n              <div class=\"item\">\r\n                <div><span></span></div>\r\n              </div>\r\n            </div>\r\n            <div class=\"feedback feedback--roadmap\">\r\n              <div class=\"feedback-items-wrapper\">\r\n                ${el.visible && filterInMap(shown[i]).join(\"\")}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        `\r\n        : null;\r\n\r\n      return el.visible\r\n        ? `\r\n        <div class=\"roadmap-mobile-header\">\r\n          ${mobileHeader()}  \r\n        </div>\r\n        ` + visibleColumn\r\n        : null;\r\n    });\r\n\r\n    return mapped.join(\"\");\r\n  };\r\n\r\n  const switchColumns = (e) => {\r\n    let current = columns.find(\r\n      (el) =>\r\n        el.name == e.currentTarget.firstElementChild.firstElementChild.innerHTML\r\n    );\r\n    columns.forEach((el) => (el.visible = false));\r\n    current.visible = true;\r\n    roadmapColumnsWrapper.innerHTML = columnsLists();\r\n    const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\r\n\r\n    columnSwitchBtns.forEach((element) => {\r\n      element.addEventListener(\"click\", switchColumns);\r\n    });\r\n    e.currentTarget.classList.add(\"btn-name--active\");\r\n    e.currentTarget.style.display = \"none\"\r\n    // add event listeners after repiant\r\n    addItemDetailsListener();\r\n  };\r\n\r\n  roadmapColumnsWrapper.innerHTML = columnsLists();\r\n  addItemDetailsListener();\r\n\r\n  // Mobile switch columns\r\n  const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\r\n\r\n  columnSwitchBtns &&\r\n    columnSwitchBtns.forEach((element) => {\r\n      element.addEventListener(\"click\", switchColumns);\r\n    });\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"9d7658d2d10ea6c078dfe03dc32bd716","cacheData":{"env":{}}}