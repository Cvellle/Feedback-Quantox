erated":{"line":1,"column":3524}},{"source":"src/assets/scripts/routes/details.js","original":{"line":108,"column":12},"generated":{"line":1,"column":3529}},{"source":"src/assets/scripts/routes/details.js","name":"image","original":{"line":109,"column":8},"generated":{"line":1,"column":3530}},{"source":"src/assets/scripts/routes/details.js","original":{"line":109,"column":15},"generated":{"line":1,"column":3536}},{"source":"src/assets/scripts/routes/details.js","name":"name","original":{"line":110,"column":8},"generated":{"line":1,"column":3577}},{"source":"src/assets/scripts/routes/details.js","original":{"line":110,"column":14},"generated":{"line":1,"column":3582}},{"source":"src/assets/scripts/routes/details.js","name":"username","original":{"line":111,"column":8},"generated":{"line":1,"column":3598}},{"source":"src/assets/scripts/routes/details.js","original":{"line":111,"column":18},"generated":{"line":1,"column":3607}},{"source":"src/assets/scripts/routes/details.js","name":"getItems","original":{"line":116,"column":4},"generated":{"line":1,"column":3623}},{"source":"src/assets/scripts/routes/details.js","name":"document","original":{"line":117,"column":18},"generated":{"line":1,"column":3627}},{"source":"src/assets/scripts/routes/details.js","name":"querySelector","original":{"line":117,"column":27},"generated":{"line":1,"column":3636}},{"source":"src/assets/scripts/routes/details.js","original":{"line":117,"column":41},"generated":{"line":1,"column":3650}},{"source":"src/assets/scripts/routes/details.js","name":"innerHTML","original":{"line":118,"column":10},"generated":{"line":1,"column":3668}},{"source":"src/assets/scripts/routes/details.js","name":"getItems","original":{"line":118,"column":22},"generated":{"line":1,"column":3678}}],"sources":{"src/assets/scripts/routes/details.js":"import { initialValues } from \"../modules/getSuggestions\";\nimport { goBack, setPreviousRoute } from \"../shared/shared-functions\";\nimport { router } from \"./router\";\n\nrouter.on(\"/item/:id\", function (match) {\n  document.body.style = \"background: #f2f2f2\";\n  let currentId = match.data.id;\n  let current = initialValues.feedbackArray.filter(\n    (el) => el.id == currentId\n  )[0];\n\n  const getItems = () => {\n    let items =\n      current.comments &&\n      current.comments.map((el) => {\n        return `\n        <div class=\"item\">\n          <div class=\"info\">\n            <div class=\"profile-image\">\n              <div cass=\"${el.user.image.split(\"/\").pop()}\"\n                 style=\"background-image: url(${el.user.image\n                   .split(\"/\")\n                   .pop()})\">\n              </div>\n            </div>\n            <div class=\"name\">\n              <bold>${el.user.name}</bold>\n              <div>@${el.user.username}</div>\n            </div>\n            <span class=\"reply-activate\">reply</span>\n          </div>\n          <div class=\"text\">${el.content}</div>\n          <div class=\"reply\">\n            <textarea placeholder=\"Type your reply here\"></textarea>\n            <button>Post Reply</button>\n          </div>\n        </div>`;\n      });\n\n    return items.join(\"\");\n  };\n\n  const detailsTemplate = `\n    <section class=\"details\">\n      <div class=\"details__controls\">\n        <div class=\"back\">\n          <span class=\"arrow\"></span>\n          <span class=\"text\">Go back</span>\n        </div>\n        <span class=\"edit-feedback\">+ Edit Feedback</span>\n      </div>\n      <div class=\"details__current\">\n        <div class=\"feedback feedback--details\">\n          <div class=\"feedback-items-wrapper\">\n            <div class=\"feedback-item\" id=\"${current.id}\">\n              <div class=\"feedback-item__left\">\n                <span class=\"arrow\"></span>\n                <div class=\"count\">${current.upvotes}</div>\n              </div>\n              <div class=\"feedback-item__center\">\n                <h4 class=\"title\">${current.title}</h4>\n                <p>\n                  ${current.description}\n                </p>\n                <div class=\"tag\">\n                  <span>${current.category}</span>\n                </div>\n              </div>\n              <div class=\"feedback-item__right\">\n                <span class=\"comment\"></span>\n                <div class=\"count\">\n                  ${current.comments ? current.comments.length : 0}\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"comments\">\n        <bold>\n          <span>${current.comments ? current.comments.length : 0}</span>\n          Comments\n        </bold>\n          <div class=\"items-wrapper\">\n            ${current.comments && getItems()}\n          </div>\n          <div class=\"add\">\n            <bold></bold>\n            <textarea name=\"\" id=\"\" cols=\"30\" rows=\"10\" placeholder=\"Type your comment here\"></textarea>\n            <div class=\"post\">\n              <span>asd lef</span>\n              <button class=\"post-comment\">Post Comment</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>`;\n\n  document.body.innerHTML = detailsTemplate;\n\n  // Functions\n  const addComment = document.querySelector(\".post-comment\");\n\n  const addCommentFunction = () => {\n    let newComment = {\n      content:\n        \"Awesome idea! Trying to find framework-specific projects within the hubs can be tedious\",\n      id: 1,\n      user: {\n        image: \"./assets/user-images/image-suzanne.jpg\",\n        name: \"Suzanne Chang\",\n        username: \"upbeat1811\",\n      },\n    };\n\n    current.comments.push(newComment);\n    getItems();\n    const items = document.querySelector(\".items-wrapper\");\n    items.innerHTML = getItems();\n  };\n\n  // set current route as back destination, and imported back function\n  setPreviousRoute(match.url);\n\n  const back = document.querySelector(\".details__controls .back\");\n  const goToEdit = document.querySelector(\".details__controls .edit-feedback\");\n\n  goToEdit.onclick = () => router.navigate(\"/item/edit/\" + currentId);\n\n  back.addEventListener(\"click\", goBack);\n  addComment.addEventListener(\"click\", addCommentFunction);\n});\n"},"lineCount":null}},"error":null,"hash":"88cf62b1279743ef34fdc838597d9ec1","cacheData":{"env":{}}}