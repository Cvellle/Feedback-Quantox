s/details.js","original":{"line":99,"column":27}},{"name":"navigate","generated":{"line":29,"column":26},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":34}},{"generated":{"line":29,"column":34},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":27}},{"generated":{"line":29,"column":35},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":43}},{"name":"currentId","generated":{"line":29,"column":51},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":59}},{"generated":{"line":29,"column":60},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":27}},{"generated":{"line":29,"column":61},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":21}},{"generated":{"line":30,"column":0},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":21}},{"generated":{"line":30,"column":3},"source":"src/assets/scripts/routes/details.js","original":{"line":99,"column":2}},{"name":"back","generated":{"line":32,"column":0},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":2}},{"name":"back","generated":{"line":32,"column":2},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":2}},{"generated":{"line":32,"column":6},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":6}},{"name":"addEventListener","generated":{"line":32,"column":7},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":7}},{"generated":{"line":32,"column":23},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":2}},{"generated":{"line":32,"column":24},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":24}},{"generated":{"line":32,"column":31},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":2}},{"name":"goBack","generated":{"line":32,"column":33},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":33}},{"generated":{"line":32,"column":56},"source":"src/assets/scripts/routes/details.js","original":{"line":101,"column":2}},{"generated":{"line":33,"column":0},"source":"src/assets/scripts/routes/details.js","original":{"line":102,"column":1}},{"generated":{"line":33,"column":1},"source":"src/assets/scripts/routes/details.js","original":{"line":6,"column":0}}],"sources":{"src/assets/scripts/routes/details.js":"import { getSuggestions, initialValues } from \"../modules/getSuggestions\";\r\nimport { goBack, setPreviousRoute } from \"../shared/shared-functions\";\r\nimport { router } from \"./router\";\r\n// import an from \"../../images/user-images/image-anne.jpg\";\r\n\r\nrouter.on(\"/item/:id\", function (match) {\r\n  document.body.style = \"background: #f2f2f2\";\r\n  let currentId = match.data.id;\r\n  let current = initialValues.feedbackArray.filter(\r\n    (el) => el.id == currentId\r\n  )[0];\r\n\r\n  let items = current.comments.map((el) => {\r\n    return `\r\n      <div class=\"item\">\r\n        <div class=\"info\">\r\n          <div class=\"profile-image\">\r\n            <div style=\"background-image: url(/src/assets/images/user-images/${el.user.image\r\n              .split(\"/\")\r\n              .pop()})\">\r\n            </div>\r\n          </div>\r\n          <div class=\"name\">\r\n            <bold>${el.user.name}</bold>\r\n            <div>@${el.user.username}</div>\r\n          </div>\r\n          <span class=\"reply-activate\">reply</span>\r\n        </div>\r\n        <div class=\"text\">${el.content}</div>\r\n        <div class=\"reply\">\r\n          <textarea placeholder=\"Type your reply here\"></textarea>\r\n          <button>Post Reply</button>\r\n        </div>\r\n      </div>`;\r\n  });\r\n\r\n  const detailsTemplate = `\r\n    <section class=\"details\">\r\n      <div class=\"details__controls\">\r\n        <div class=\"back\">\r\n          <span class=\"arrow\"></span>\r\n          <span class=\"text\">Go back</span>\r\n        </div>\r\n        <span class=\"edit-feedback\">+ Edit Feedback</span>\r\n      </div>\r\n      <div class=\"details__current\">\r\n        <div class=\"feedback feedback--details\">\r\n          <div class=\"feedback-items-wrapper\">\r\n            <div class=\"feedback-item\" id=\"${current.id}\">\r\n              <div class=\"feedback-item__left\">\r\n                <span class=\"arrow\"></span>\r\n                <div class=\"count\">${current.upvotes}</div>\r\n              </div>\r\n              <div class=\"feedback-item__center\">\r\n                <h4 class=\"title\">${current.title}</h4>\r\n                <p>\r\n                  ${current.description}\r\n                </p>\r\n                <div class=\"tag\">\r\n                  <span>${current.category}</span>\r\n                </div>\r\n              </div>\r\n              <div class=\"feedback-item__right\">\r\n                <span class=\"comment\"></span>\r\n                <div class=\"count\">\r\n                  ${current.comments ? current.comments.length : 0}\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"comments\">\r\n        <bold>\r\n          <span>${current.comments ? current.comments.length : 0}</span>\r\n          Comments\r\n        </bold>\r\n          <div class=\"items-wrapper\">\r\n            ${items.join(\"\")}\r\n          </div>\r\n          <div class=\"add\">\r\n            <bold></bold>\r\n            <textarea name=\"\" id=\"\" cols=\"30\" rows=\"10\" placeholder=\"Type your comment here\"></textarea>\r\n            <div class=\"post\">\r\n              <span>asd lef</span>\r\n              <button>Post Comment</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>`;\r\n\r\n  document.body.innerHTML = detailsTemplate;\r\n\r\n  // set current route as back destination, and imported back function\r\n  setPreviousRoute();\r\n  const back = document.querySelector(\".details__controls .back\");\r\n  const goToEdit = document.querySelector(\".details__controls .edit-feedback\");\r\n\r\n  goToEdit.onclick = () => router.navigate(\"/item/edit/\" + currentId)\r\n\r\n  back.addEventListener(\"click\", goBack);\r\n});\r\n"},"lineCount":null}},"error":null,"hash":"9f34e071338da7142ceb1547c9274314","cacheData":{"env":{}}}