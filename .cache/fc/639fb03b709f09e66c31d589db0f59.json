umn":61}},{"name":"comments","generated":{"line":21,"column":1865},"source":"src/assets/scripts/templates/details.template.js","original":{"line":84,"column":62}},{"generated":{"line":21,"column":1873},"source":"src/assets/scripts/templates/details.template.js","original":{"line":84,"column":47}},{"generated":{"line":21,"column":1875},"source":"src/assets/scripts/templates/details.template.js","original":{"line":84,"column":72}},{"generated":{"line":21,"column":1884},"source":"src/assets/scripts/templates/details.template.js","original":{"line":84,"column":47}},{"generated":{"line":21,"column":1885},"source":"src/assets/scripts/templates/details.template.js","original":{"line":84,"column":14}},{"generated":{"line":21,"column":1888},"source":"src/assets/scripts/templates/details.template.js","original":{"line":84,"column":85}},{"generated":{"line":21,"column":1890},"source":"src/assets/scripts/templates/details.template.js","original":{"line":43,"column":2}},{"generated":{"line":22,"column":0},"source":"src/assets/scripts/templates/details.template.js","original":{"line":97,"column":1}},{"generated":{"line":22,"column":1},"source":"src/assets/scripts/templates/details.template.js","original":{"line":41,"column":7}}],"sources":{"src/assets/scripts/templates/details.template.js":"\r\n\r\nexport const getItems = (toMap, elementKind) => {\r\n  let items =\r\n      toMap && toMap.map((el, i) => {\r\n      // let a = () => require('/src/assets/images/' + el.user.image.substring(1).split('/').slice(2).join('/'))\r\n      // let a = async () => await import('/src/assets/images/' + el.user.image.substring(1).split('/').slice(2).join('/'))    \r\n      return `\r\n        <div class=\"item item--${elementKind}\" data-index=\"${i}\">\r\n          <div class=\"info\">\r\n            <div class=\"profile-image\">\r\n              <div cass=\"${el && el.user && el.user.image && el.user.image.split(\"/\").pop()}\"\r\n                style=\"background-image: url(${el && el.user && el.user.image\r\n                  .split(\"/\")\r\n                  .pop()})\">\r\n              </div>\r\n            </div>\r\n            <div class=\"name\">\r\n              <bold>${el && el.user.name}</bold>\r\n              <div>@${el && el.user.username}</div>\r\n            </div>\r\n            <span class=\"reply-activate\">reply</span>\r\n          </div>\r\n          <div class=\"text\">\r\n            <span>${!el.replies && el.replyingTo ? '@' + el.replyingTo : ''}</span>\r\n            ${el && el.content}\r\n          </div>\r\n          <div class=\"reply\">\r\n            <div class=\"${!el.replies ? 'replies-wrapper' : ''}\">\r\n              ${el.replies ? getItems(el.replies, 'reply') : ''}\r\n            </div>\r\n            <textarea placeholder=\"Type your reply here\"></textarea>\r\n            <button class=\"reply-btn\" data-reply-to=\"${el.user.username}\">Post Reply</button>\r\n          </div>\r\n        </div>`;\r\n    });\r\n\r\n  return items && items.join(\"\");\r\n};\r\n\r\nexport const detailsTemplate = (passedCurrent) => {\r\n  getItems(passedCurrent && passedCurrent.comments, 'comment');\r\n  return `\r\n    <section class=\"details\">\r\n      <div class=\"details__controls\">\r\n        <div class=\"back\">\r\n          <span class=\"arrow\"></span>\r\n          <span class=\"text\">Go back</span>\r\n        </div>\r\n        <span class=\"edit-feedback\">+ Edit Feedback</span>\r\n      </div>\r\n      <div class=\"details__current\">\r\n        <div class=\"feedback feedback--details\">\r\n          <div class=\"feedback-items-wrapper\">\r\n            <div class=\"feedback-item\" id=\"${passedCurrent.id}\">\r\n              <div class=\"feedback-item__left\">\r\n                <span class=\"arrow\"></span>\r\n                <div class=\"count\">${passedCurrent.upvotes}</div>\r\n              </div>\r\n              <div class=\"feedback-item__center\">\r\n                <h4 class=\"title\">${passedCurrent.title}</h4>\r\n                <p>\r\n                  ${passedCurrent.description}\r\n                </p>\r\n                <div class=\"tag\">\r\n                  <span>${passedCurrent.category}</span>\r\n                </div>\r\n              </div>\r\n              <div class=\"feedback-item__right\">\r\n                <span class=\"comment\"></span>\r\n                <div class=\"count count-comments\">\r\n                  ${passedCurrent.comments ? passedCurrent.comments.length : 0}\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"comments\">\r\n        <bold>\r\n          <span class=\"count-comments\">${passedCurrent.comments ? passedCurrent.comments.length : ''}</span>\r\n          <span>${passedCurrent.comments && passedCurrent.comments.length == 1 ? \"Comment\" : \"Comments\"}</span>\r\n        </bold>\r\n          <div class=\"items-wrapper\">\r\n            ${passedCurrent.comments ? getItems(passedCurrent.comments, 'comment') : ''}\r\n          </div>\r\n          <div class=\"add\">\r\n            <bold>Add Comments</bold>\r\n            <textarea name=\"\" id=\"\" cols=\"30\" rows=\"10\" placeholder=\"Type your comment here\"></textarea>\r\n            <div class=\"post\">\r\n              <span><span class=\"char-left\" max=\"250\">250</span> characters left</span>\r\n              <button class=\"post-comment\">Post Comment</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>`;\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"3f42c2a60bd3bce796c497f32895d5c7","cacheData":{"env":{}}}