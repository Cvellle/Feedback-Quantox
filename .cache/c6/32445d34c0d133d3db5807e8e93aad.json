olumn":0}},{"name":"window","generated":{"line":87,"column":0},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":0}},{"generated":{"line":87,"column":6},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":6}},{"name":"addEventListener","generated":{"line":87,"column":7},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":7}},{"generated":{"line":87,"column":23},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":0}},{"generated":{"line":87,"column":24},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":24}},{"generated":{"line":87,"column":34},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":0}},{"name":"fetchSuggestions","generated":{"line":87,"column":36},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":36}},{"generated":{"line":87,"column":52},"source":"src/assets/scripts/modules/getSuggestions.js","original":{"line":77,"column":0}}],"sources":{"src/assets/scripts/modules/getSuggestions.js":"import { router } from \"../routes/router\";\nimport data from \"/src/data/data\";\nimport root from \"../routes/root\";\nimport \"/src/assets/scripts/routes/details\";\n\nexport const initialValues = {\n  feedbackArray: null,\n  currentUser: null,\n};\n\nexport const getSuggestions = (arrayToLoop, toFilter) => {\n  const feedbackWrapper = document.querySelector(\".feedback-items-wrapper\");\n  // const sidebarStatusDisplay = document.querySelector(\".sidebar__status-display\");\n\n  const suggestionsList = arrayToLoop.filter((el) => {\n    let final = toFilter ? el.id == toFilter : el;\n\n    return final;\n  });\n\n  let mapped = suggestionsList.map((el) => {\n    return `<div class=\"feedback-item\" id=\"${el.id}\">\n              <div class=\"feedback-item__left\">\n                <span class=\"arrow\"></span>\n                <div class=\"count\">${el.upvotes}</div>\n              </div>\n              <div class=\"feedback-item__center\">\n                <h4 class=\"title\">${el.title}</h4>\n                <p>\n                  ${el.description}\n                </p>\n                <div class=\"tag\">\n                  <span>${el.category}</span>\n                </div>\n              </div>\n              <div class=\"feedback-item__right\">\n                <span class=\"comment\"></span>\n                <div class=\"count\">\n                  ${el.comments ? el.comments.length : 0}\n                </div>\n              </div>\n            </div>`;\n  });\n\n  // fill the container\n  feedbackWrapper.innerHTML = mapped.join(\"\");\n\n  // item recognition\n  const feedbackItems = document.querySelectorAll(\".feedback-item\");\n  feedbackItems &&\n    feedbackItems.forEach((element) => {\n      element.addEventListener(\"click\", feedbackDetails);\n    });\n  function feedbackDetails(e) {\n    router.navigate(\"/item/\" + e.currentTarget.id);\n\n    getSuggestions(initialValues.feedbackArray, e.currentTarget.id);\n  }\n};\n\nasync function fetchSuggestions() {\n  const feedbackWrapper = document.querySelector(\".feedback-items-wrapper\");\n\n  // fetch\n  // const response = await fetch(\"/data/data.json\");\n  // const json = await response.json();\n\n  // fill the object\n  initialValues.feedbackArray = data.productRequests;\n\n  //call outer getSuggestions function\n  getSuggestions(data.productRequests);\n}\n\n// module invoked on load\nwindow.addEventListener(\"load\", fetchSuggestions);\nwindow.addEventListener(\"popstate\", fetchSuggestions);\n"},"lineCount":null}},"error":null,"hash":"c47f71d031bc86f82b21660d0ca81663","cacheData":{"env":{}}}