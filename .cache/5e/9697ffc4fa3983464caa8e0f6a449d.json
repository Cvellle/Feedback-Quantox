lumn":4},"generated":{"line":1,"column":3237}},{"source":"src/assets/scripts/modules/roadmap.js","name":"forEach","original":{"line":144,"column":21},"generated":{"line":1,"column":3239}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":144,"column":29},"generated":{"line":1,"column":3247}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":144,"column":30},"generated":{"line":1,"column":3256}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":145,"column":6},"generated":{"line":1,"column":3259}},{"source":"src/assets/scripts/modules/roadmap.js","name":"addEventListener","original":{"line":145,"column":14},"generated":{"line":1,"column":3261}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":145,"column":31},"generated":{"line":1,"column":3278}},{"source":"src/assets/scripts/modules/roadmap.js","name":"switchColumns","original":{"line":145,"column":40},"generated":{"line":1,"column":3286}}],"sources":{"src/assets/scripts/modules/roadmap.js":"import { tabletMin } from \"../shared/constants\";\nimport {\n  addItemDetailsListener,\n  feedbackDetails,\n  filterStatus,\n} from \"../shared/shared-functions\";\nimport { getLS, initialValues } from \"./getSuggestions\";\n\nexport function roadmapLists() {\n  const roadmapFeedbackWrapper = document.querySelector(\n    \".feedback--roadmap .feedback-items-wrapper\"\n  );\n  const feedbackItems = document.querySelectorAll(\".feedback-item\");\n  const roadmapColumnsWrapper = document.querySelector(\n    \".roadmap .roadmap__columns\"\n  );\n\n  const columns = [\n    {\n      name: \"planned\",\n      visible: true,\n    },\n    {\n      name: \"in-progress\",\n      visible: tabletMin,\n    },\n    {\n      name: \"live\",\n      visible: tabletMin,\n    },\n  ];\n\n  let shown = [];\n  const all = initialValues.feedbackArray;\n\n  const filterInMap = (toFilter) =>\n    toFilter.map((el, i) => {\n      let currentObject = getLS(\"feedbackArray\")[el.id - 1];\n      return `<div class=\"feedback-item\" id=\"${el.id}\">\n                <div class=\"feedback-item__left\">\n                  <div class=\"upvotes ${\n                    currentObject.likedBy &&\n                    currentObject.likedBy.includes(getLS(\"currentUser\").name)\n                      ? \"upvotes--highlighted\"\n                      : \"\"\n                  }\">\n                    <span class=\"arrow\"></span>\n                    <input type=\"hidden\"/>\n                    <div class=\"count\">${el.upvotes}</div>\n                </div>\n                </div>\n                <div class=\"feedback-item__center\">\n                  <h4 class=\"title\">${el.title}</h4>\n                  <p>\n                    ${el.description}\n                  </p>\n                  <div class=\"tag\">\n                    <span>${el.category}</span>\n                  </div>\n                </div>\n                <div class=\"feedback-item__right\">\n                  <span class=\"comment\"></span>\n                  <div class=\"count\">\n                    ${el.comments ? el.comments.length : 0}\n                  </div>\n                </div>\n            </div>`;\n    });\n\n  const columnsLists = () => {\n    let mapped = columns.map((el, i, self) => {\n      shown.push(all.filter((f) => f.status == el.name));\n\n      const visibleColumn = el.visible\n        ? `\n      <div class=\"roadmap__column roadmap__column--${el.name}\">\n        <bold>\n          ${el.name}\n          <span>(<span class=\"count\">${shown[i].length}</span>)</span>\n        </bold>\n        <div>In search</div>\n        <div>\n          <div class=\"item\">\n            <div><span></span></div>\n          </div>\n        </div>\n        <div class=\"feedback feedback--roadmap\">\n          <div class=\"feedback-items-wrapper\">\n            ${el.visible && filterInMap(shown[i]).join(\"\")}\n          </div>\n        </div>\n      </div>\n    `\n        : null;\n\n      const mobileHeader = self.map((buttonEl, buttonIndex) => {\n        let allColumns = filterStatus(true);\n        return `\n        <div class=\"btn-name\">\n          <bold>\n            <span>${buttonEl.name}</span>\n            <span>(<span class=\"count\">${allColumns[buttonIndex].length}</span>)</span>\n          </bold>\n        </div> `;\n      });\n\n      return el.visible\n        ? `\n      <div class=\"roadmap-mobile-header\">\n        ${mobileHeader.join(\"\")}  \n      </div>\n    ` + visibleColumn\n        : null;\n    });\n\n    return mapped.join(\"\");\n  };\n\n  const switchColumns = (e) => {\n    let current = columns.find(\n      (el) =>\n        el.name == e.currentTarget.firstElementChild.firstElementChild.innerHTML\n    );\n    columns.forEach((el) => (el.visible = false));\n    current.visible = true;\n    roadmapColumnsWrapper.innerHTML = columnsLists();\n    const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\n\n    columnSwitchBtns.forEach((element) => {\n      element.addEventListener(\"click\", switchColumns);\n    });\n    e.currentTarget.classList.add(\"btn-name--active\");\n    // add event listeners after repiant\n    addItemDetailsListener();\n  };\n\n  roadmapColumnsWrapper.innerHTML = columnsLists();\n  addItemDetailsListener();\n\n  // Mobile switch columns\n  const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\n\n  columnSwitchBtns &&\n    columnSwitchBtns.forEach((element) => {\n      element.addEventListener(\"click\", switchColumns);\n    });\n}\n"},"lineCount":null}},"error":null,"hash":"62d0ce5d884a5b426f78099fdc730833","cacheData":{"env":{}}}