dmap.js","name":"document","original":{"line":129,"column":27},"generated":{"line":1,"column":2894}},{"source":"src/assets/scripts/modules/roadmap.js","name":"querySelectorAll","original":{"line":129,"column":36},"generated":{"line":1,"column":2903}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":129,"column":53},"generated":{"line":1,"column":2920}},{"source":"src/assets/scripts/modules/roadmap.js","name":"columnSwitchBtns","original":{"line":131,"column":2},"generated":{"line":1,"column":2933}},{"source":"src/assets/scripts/modules/roadmap.js","name":"columnSwitchBtns","original":{"line":132,"column":4},"generated":{"line":1,"column":2936}},{"source":"src/assets/scripts/modules/roadmap.js","name":"forEach","original":{"line":132,"column":21},"generated":{"line":1,"column":2938}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":132,"column":29},"generated":{"line":1,"column":2946}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":132,"column":30},"generated":{"line":1,"column":2955}},{"source":"src/assets/scripts/modules/roadmap.js","name":"element","original":{"line":133,"column":6},"generated":{"line":1,"column":2958}},{"source":"src/assets/scripts/modules/roadmap.js","name":"addEventListener","original":{"line":133,"column":14},"generated":{"line":1,"column":2960}},{"source":"src/assets/scripts/modules/roadmap.js","original":{"line":133,"column":31},"generated":{"line":1,"column":2977}},{"source":"src/assets/scripts/modules/roadmap.js","name":"switchColumns","original":{"line":133,"column":40},"generated":{"line":1,"column":2985}}],"sources":{"src/assets/scripts/modules/roadmap.js":"import { feedbackDetails, filterStatus } from \"../shared/shared-functions\";\nimport { initialValues } from \"./getSuggestions\";\n\nexport function roadmapLists() {\n  const roadmapFeedbackWrapper = document.querySelector(\n    \".feedback--roadmap .feedback-items-wrapper\"\n  );\n  const feedbackItems = document.querySelectorAll(\".feedback-item\");\n  const roadmapColumnsWrapper = document.querySelector(\n    \".roadmap .roadmap__columns\"\n  );\n\n  const columns = [\n    {\n      name: \"planned\",\n      visible: true,\n    },\n    {\n      name: \"in-progress\",\n      visible: false,\n    },\n    {\n      name: \"live\",\n      visible: false,\n    },\n  ];\n\n  let shown = [];\n  const all = initialValues.feedbackArray;\n\n  const filterInMap = (toFilter) =>\n    toFilter.map((el) => {\n      return `<div class=\"feedback-item\" id=\"${el.id}\">\n                <div class=\"feedback-item__left\">\n                  <span class=\"arrow\"></span>\n                  <div class=\"count\">${el.upvotes}</div>\n                </div>\n                <div class=\"feedback-item__center\">\n                  <h4 class=\"title\">${el.title}</h4>\n                  <p>\n                    ${el.description}\n                  </p>\n                  <div class=\"tag\">\n                    <span>${el.category}</span>\n                  </div>\n                </div>\n                <div class=\"feedback-item__right\">\n                  <span class=\"comment\"></span>\n                  <div class=\"count\">\n                    ${el.comments ? el.comments.length : 0}\n                  </div>\n                </div>\n            </div>`;\n    });\n\n  const columnsLists = (toMap) => {\n    let mapped = toMap.map((el, i, self) => {\n      shown.push(all.filter((f) => f.status == el.name));\n      const visibleColumn = el.visible\n        ? `\n      <div class=\"roadmap__column roadmap__column--${el.name}\">\n        <bold>\n          ${el.name}\n          <span>(<span class=\"count\">${shown[i].length}</span>)</span>\n        </bold>\n        <div>In search</div>\n        <div>\n          <div class=\"item\">\n            <div><span></span></div>\n          </div>\n        </div>\n        <div class=\"feedback feedback--roadmap\">\n          <div class=\"feedback-items-wrapper\">\n            ${filterInMap(shown[i]).join(\"\")}\n          </div>\n        </div>\n      </div>\n    `\n        : null;\n\n      const mobileHeader = self.map((buttonEl, buttonIndex) => {\n        let allColumns = filterStatus(true);\n        return `\n        <div class=\"btn-name\">\n          <bold>\n            <span>${buttonEl.name}</span>\n            <span>(<span class=\"count\">${allColumns[buttonIndex].length}</span>)</span>\n          </bold>\n        </div> `;\n      });\n\n      return el.visible\n        ? `\n      <div class=\"roadmap-mobile-header\">\n        ${mobileHeader.join(\"\")}  \n      </div>\n    ` + visibleColumn\n        : null;\n    });\n\n    return mapped.join(\"\");\n  };\n\n  roadmapColumnsWrapper.innerHTML = columnsLists(columns);\n\n  const switchColumns = (e) => {\n    let current = columns.find(\n      (el) =>\n        el.name == e.currentTarget.firstElementChild.firstElementChild.innerHTML\n    );\n    columns.forEach((el) => (el.visible = false));\n    current.visible = true;\n    roadmapColumnsWrapper.innerHTML = columnsLists(columns);\n    const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\n\n    columnSwitchBtns.forEach((element) => {\n      element.addEventListener(\"click\", switchColumns);\n    });\n    e.currentTarget.classList.add(\"btn-name--active\");\n  };\n\n  // Add event listeners\n  // item recognition\n  feedbackItems &&\n    feedbackItems.forEach((element) => {\n      element.addEventListener(\"click\", feedbackDetails);\n    });\n  // Mobile switch columns\n  const columnSwitchBtns = document.querySelectorAll(\".btn-name\");\n\n  columnSwitchBtns &&\n    columnSwitchBtns.forEach((element) => {\n      element.addEventListener(\"click\", switchColumns);\n    });\n}\n"},"lineCount":null}},"error":null,"hash":"5874e9412af164f45baaceb662ff2a01","cacheData":{"env":{}}}